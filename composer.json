{
    "name": "wnx/laravel-download-statistics",
    "type": "project",
    "description": "Laravel application to gather download statistics for Laravel.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "blade-ui-kit/blade-icons": "^1.5",
        "itsgoingd/clockwork": "^5.0",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.0",
        "laravel/tinker": "^2.0",
        "nikolaposa/version": "^4.0",
        "predis/predis": "^2.1",
        "spatie/laravel-export": "^0.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.18.12",
        "spatie/laravel-ignition": "^2.0",
        "wnx/laravel-stats": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/FormattingHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "format": "php-cs-fixer fix",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
